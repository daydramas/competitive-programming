#include <bits/stdc++.h>
using namespace std;

// structures and defines
#define pii pair<int, int>
// global variables

// functions
void solve(int t) {

	int N, M, X, C[1000];
	vector<pii> adj[1000];

	for (int i=0; i<N; i++) cin >> C[i];
	for (int i=0; i<M; i++) {
		int a, b, c; cin >> a >> b >> c;
		adj[a].push_back(pii(b, c));
		adj[b].push_back(pii(a, c));
	}
}
int main() {
	// local variables
	int T;

	// input
	cin >> T;

	// main
	for (int i=1; i<=T; i++) {
		solve(i);
	}

	// output

	return 0;
}
