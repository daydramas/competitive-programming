#define NO() cout <<"NO"<<endl;
ll gcd(ll a, ll b) {
    if (b == 0) return a;
    return gcd(b, a % b);
}
ll lcm(ll a, ll b) { return (a*b)/gcd(a, b); }
void primeFactors(ll n) {
    while (n % 2 == 0)  {
    	// 2 is factor
    	n = n/2;
    }
    for (int i = 3; i <= sqrt(n); i = i + 2)  {
        while (n % i == 0)  {
        	// i is factor
            n = n/i;
        }
    }
    if (n > 2)  {
    	// n is factor
    }
}
